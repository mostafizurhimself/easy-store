{
    "name": "easystore/invoice",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Easystore\\Invoice\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Easystore\\Invoice\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
